{
  "name": "playground-universal-react",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "run-p start:*",
    "start:css": "cross-env NODE_ENV=development postcss './src/styles/*.css' --dir './assets' -w",
    "start:client": "cross-env NODE_ENV=development webpack --watch --config tools/webpack/server/development",
    "storybook": "cross-env NODE_ENV=production start-storybook -p 9001 -c .storybook",
    "prebuild": "npm run clean:build",
    "build": "run-s build:*",
    "build:css": "cross-env NODE_ENV=production postcss './src/styles/*.css' --dir './assets' --no-map",
    "build:client": "cross-env NODE_ENV=production webpack --progress --config tools/webpack/client/production.js",
    "build:server": "cross-env NODE_ENV=production webpack --config tools/webpack/server/production",
    "server": "cross-env NODE_ENV=production node build/server.js",
    "clean": "run-p clean:*",
    "clean:dev": "rimraf .tmp",
    "clean:build": "rimraf build",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "safe-guard": "lint-staged",
    "lint": "run-p lint:*",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:script": "tslint -t stylish 'src/**/*.{ts,tsx}'",
    "fix": "run-p fix:*",
    "fix:style": "prettier --write 'src/**/*.css'",
    "fix:script": "tslint -t stylish --fix 'src/**/*.{ts,tsx}'",
    "fix:json": "prettier --write 'src/**/*.json'",
    "typecheck": "tsc --noEmit -p tools/tsconfig/server.production.json"
  },
  "browserslist": [
    "> 0.5% in jp",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.{ts,tsx}": [
        "tslint --fix",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s typecheck safe-guard"
    }
  },
